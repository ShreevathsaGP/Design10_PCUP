<HomeScreen>:

    orientation: 'vertical'
    canvas:
        Color:
            rgba: (1,1,1,1)
        Rectangle:
            pos: self.pos
            size: self.size

    name: "home"

    GridLayout:
        size: root.width, root.height
        cols:1

        GridLayout:
            cols:1
            Button:
                id: generate_btn_1
                on_release:
                    app.root.current = "educate"
                    root.manager.transition.direction = "left"
                background_color: (255,255,255)
                Image:
                    allow_stretch: True
                    keep_ratio: False
                    size: generate_btn_1.width, generate_btn_1.height
                    source: 'DESIGN_PCUP_1.png'
                    pos: generate_btn_1.pos

        GridLayout:
            id: generate_btn_2
            cols:1
            Button:
                on_release:
                    app.root.current = "calculate"
                    root.manager.transition.direction = "left"
                background_color: (255,255,255)
                Image:
                    allow_stretch: True
                    keep_ratio: False
                    size: generate_btn_2.size
                    source: 'DESIGN_PCUP_2.png'
                    pos: generate_btn_2.pos
        GridLayout:
            id: generate_btn_3
            cols:1
            Button:
                on_release:
                    app.root.current = "take_action"
                    root.manager.transition.direction = "left"
                Image:
                    allow_stretch: True
                    keep_ratio: False
                    size: generate_btn_3.size
                    source: 'DESIGN_PCUP_3.png'
                    pos: generate_btn_3.pos


<Educate>:
    info_1: info_1

    orientation: 'vertical'
    spacing: 10
    canvas:
        Color:
            rgb: (1,1,1,1)
        Rectangle:
            pos: self.pos
            size: self.size
    name: "educate"

    NavigationDrawer:
        id: main_win
        BoxLayout:
            canvas:
                Color:
                    rgb: (1,1,1,1)
                Rectangle:
                    pos: self.pos
                    size: self.size
            GridLayout:
                cols:1
                Label:
                    text: 'Climate Change Guide'
                    color: 0,0,0,1
                    size_hint_y: 0.2
                    size_hint_x:1

                Button:
                    size_hint_y: 0.1
                    size_hint_x:1
                    background_color: (0, 255, 255, 1)
                    text: 'What is climate change?'
                    color: (0,0,0,1)

                Button:
                    size_hint_y: 0.1
                    size_hint_x:1
                    background_color: (128, 0, 0, 0.6)
                    text: 'Greenhouse Effect'
                    color: (0,0,0,1)
                    on_release:
                        app.root.current = 'educate_2'
                        root.manager.transition.direction = 'up'
                Button:
                    size_hint_y: 0.1
                    size_hint_x:1
                    background_color: (128, 0, 0, 0.6)
                    text: 'Impacts of Climate Change'
                    color: (0,0,0,1)
                    on_release:
                        app.root.current = 'educate_3'
                        root.manager.transition.direction = 'up'
                Button:
                    size_hint_y: 0.1
                    size_hint_x:1
                    background_color: (128, 0, 0, 0.6)
                    text: 'Coping with the impacts'
                    color: (0,0,0,1)
                    on_release:
                        app.root.current = 'educate_4'
                        root.manager.transition.direction = 'up'
                Button:
                    size_hint_y: 0.1
                    size_hint_x:1
                    background_color: (128, 0, 0, 0.6)
                    color: (0,0,0,1)
                    text: 'Works Cited'
                    color: (0,0,0,1)
                    on_release:
                        app.root.current = 'educate_5'
                        root.manager.transition.direction = 'up'
                Button:
                    size_hint_y: 0.1
                    size_hint_x:1
                    background_color: (255,255,255,1)
                    text: 'no'

                Button:
                    size_hint_y: 0.1
                    size_hint_x:1
                    background_color: (255,255,255,1)
                    text: 'no'

                Button:
                    size_hint_y: 0.1
                    size_hint_x:1
                    background_color: (255,255,255,1)
                    text: 'no'

        GridLayout:
            cols:1
            canvas:
                Color:
                    rgb: (1,1,1,1)
                Rectangle:
                    pos: self.pos
                    size: self.size
            BoxLayout:
                size_hint_y: 0.05

                Button:
                    background_color: (0,255,0,0.8)
                    size_hint_x:0.1
                    text: "+"
                    color: (0,0,0,1)
                    on_release: main_win.toggle_state()
                Label:
                    text: "Climate Change Guide"
                    color: (0,0,0,1)
                    bold: True
                    size_hint_x: 0.7
                    canvas.before:
                        Color:
                            rgba: 128,0,0,0.6
                        Rectangle:
                            pos: self.pos
                            size: self.size

                Button:
                    background_color: (1,0,1,1)
                    text: "Home"
                    size_hint_x: 0.2
                    on_release:
                        app.root.current = "home"
                        root.manager.transition.direction = "right"

            GridLayout:
                size_hint_y:0.95
                cols:1
                GridLayout:
                    id: graph
                    cols:1
                    size_hint_y: 0.4
                    Image:
                        allow_stretch: True
                        keep_ratio: True
                        size: graph.size
                        source: 'graph.png'
                        pos: graph.pos

                ScrollView:
                    size_hint_y: 0.6
                    Label:
                        id: info_1
                        size_hint_y: None
                        text_size: self.width, None
                        height: self.texture_size[1]
                        text: 'placeholder'
                        color: (0,0,0,1)

