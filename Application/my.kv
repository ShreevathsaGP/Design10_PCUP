<Calculate>:
    orientation: 'vertical'
    spacing: 10
    canvas:
        Color:
            rgb: (1,1,1,1)
        Rectangle:
            source:'Household_Section_Outline.png'
            pos: self.pos
            size: self.size
    name: "calculate"

    family_members: family_members
    electricity_small: electricity_small
    electricity_medium: electricity_medium
    electricity_large: electricity_large
    lpg: lpg
    no_cars: no_cars
    kmperl: kmperl
    mileage: mileage

    GridLayout:
        cols:1
        BoxLayout
            size_hint_y: 0.05
            Label:
                text: "Household Section"
                color: (0,0,0,1)
                bold: True
                size_hint_x: 0.6
                canvas.before:
                    Color:
                        rgba: 128,0,0,0.6
                    Rectangle:
                        pos: self.pos
                        size: self.size

            Button:
                background_color: (1,0,1,1)
                text: "Home"
                size_hint_x: 0.2
                on_release:
                    app.root.current = "home"
                    root.manager.transition.direction = "right"

            Button:
                background_color: (0,255,0,0.8)
                text:"Next"
		        color:(0,0,0,1)
                size_hint_x: 0.2
                on_release:
                    app.root.current = "calculate_personal"
                    root.manager.transition.direction = "left"
                    root.get_values()


        BoxLayout:
            size_hint_y:0.15833333
            BoxLayout:
                size_hint_x:0.5
                cols:1
                Label:
                    text_size: self.width, None
                    height: self.texture_size[1]
                    text: "How many members stay in your household?"
                    valign: 'middle'
                    color: (0,0,0,1)
            BoxLayout:
                size_hint_x: 0.5
                TextInput:
                    input_filter: "int"
                    id: family_members
                    multiline: False

        BoxLayout:
            size_hint_y: 0.15833333
            BoxLayout:
                size_hint_x: 0.5
                Label:
                    text: "Annual electricity usage:"
                    valign: 'middle'
                    text_size: self.width, self.height
                    height: self.texture_size[1]

                    color: (0,0,0,1)
